TestSuite suite1 {
	TestCase case1 {	
		browser.open()
		browser.goTo("http://www.imt-atlantique.fr/fr")
		page.contains(link(content:"Toutes les actualités"))
	}
	
	TestCase case2 {	
		browser.open()
		browser.goTo("http://www.imt-atlantique.fr/fr")
		link(content:"Toutes les actualités").click()
		page.contains(link(content:"Accueil"))
	}
	
	TestCase case3 {
		browser.open()
		browser.goTo("http://www.imt-atlantique.fr/fr")
		link(label:"Toutes les actualités").click()
		image(alt:"Accueil").click()
		page.contains(link(content:"Toutes les actualités"))
	}
	
	TestCase case4 {
		browser.open()
		browser.goTo("http://www.imt-atlantique.fr/fr/echercher")
		field(id:"edit-rechercher").write("Donald Trump")
		button(value:"Appliquer les filtres").click()
		page.contains(div(content:"Toutes les actualités"))
	}
	
	TestCase case5 {
		browser.open()
		browser.goTo("http://www.imt-atlantique.fr/fr/formation/trouver-ma-formation")
		checkbox(ALL).uncheck()
		//checkbox(label:"Anglais"|"A domicile"|"Temps plein").check()
		checkbox(label:"Anglais").check()
		checkbox(label:"A domicile").check()
		checkbox(label:"Temps plein").check()
		button(value:"Appliquer les critéres").click()
		page.contains(div(content:"No found course matching your criteria."))
	
	}
	
	TestCase case6 {
		browser.open()
		browser.goTo("http://www.imt-atlantique.fr/fr/rechercher")
		field(label:"Rechercher").write("2007")
		combobox(label:"Période de publication").select("Le mois dernier")
		button(value:"Appliquer les filtres").click()
		page.contains(div(content:"Aucun résultat ne correspond à votre recherche."))
	}
	
	TestCase case7 {
		browser.open()
		browser.goTo("http://www.imt-atlantique.fr/fr")
		div(class:"actu_home_ctner_inner_cell1_titre")[1].click()
		myTitle = div(class:"actu_home_ctner_inner_cell1_titre")[1].content
		page.contains(title(content:myTitle))
	}
	
	TestCase case8 {
        browser.open()
        browser.goTo("http://www.imt-atlantique.fr/fr")
        myTitle = div(class:"actu_home_ctner_inner_cell1_titre")[2].content
        myUrl = div(class:"actu_home_ctner_inner_cell1_titre")[2].href
        browser.goTo("http://www.imt-atlantique.fr/fr/echercher")
        field(id:"edit-rechercher").write(myTitle)
        button(value:"Appliquer les filtres").click()
        page.contains(link(href:myUrl))
    }
}






